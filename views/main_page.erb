<html>
  <head>
  <link rel="stylesheet" href="style.css">
  <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.11.2/jquery.min.js"></script>
  </head>
  <body>
    <div id="outdoor"></div>
    <div id="indoor"></div>
    <div id="alert"></div>
      <script>
      $(document).ready(function(){
        $('#outdoor').load('outdoor');
        setInterval(function(){
          $('#outdoor').load('outdoor');
        }, 300000);
});
      </script>
      <script>
      $(document).ready(function(){
        $('#indoor').load('indoor');
        setInterval(function(){
          $('#indoor').load('indoor');
        }, 300000);
});
      </script>
      <script>
      $(document).ready(function(){
        $('#alert').load('alert');
        setInterval(function(){
          $('#alert').load('alert');
        }, 300000);
});
      </script>
  </body>
</html>
